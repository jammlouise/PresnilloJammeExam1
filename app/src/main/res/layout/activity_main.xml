<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_bright"
    tools:context=".MainActivity">

    <View
        android:id="@+id/divider2"
        android:layout_width="296dp"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="216dp"
        android:background="@android:color/background_light" />

    <View
        android:id="@+id/divider4"
        android:layout_width="296dp"
        android:layout_height="3dp"
        android:layout_below="@+id/imageButton6"
        android:layout_centerHorizontal="true"
        android:background="@android:color/background_light" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="150dp"
        android:layout_height="133dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/divider2"
        android:layout_marginBottom="32dp"
        android:background="@android:color/holo_red_light"
        android:text="Note 1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="152dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/divider2"
        android:layout_marginTop="233dp"
        android:background="#faeec0"
        android:text="Note 1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="60dp"
        android:layout_height="41dp"
        android:layout_alignStart="@+id/imageButton8"
        android:layout_alignTop="@+id/textView4"
        android:gravity="center"
        android:text="Public Notes"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="60dp"
        android:layout_height="41dp"
        android:layout_alignTop="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="Office Notes"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="60dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="160dp"
        android:gravity="center"
        android:text="Personal Notes"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="132dp"
        android:layout_height="175dp"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignEnd="@+id/divider2"
        android:background="@android:color/holo_orange_light" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="38dp"
        android:layout_height="37dp"
        android:layout_above="@+id/divider4"
        android:layout_alignEnd="@+id/divider5"
        android:background="@null"
        app:srcCompat="@android:drawable/ic_menu_edit" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="47dp"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@null"
        app:srcCompat="?attr/homeAsUpIndicator" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="69dp"
        android:layout_height="62dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="88dp"
        app:srcCompat="@android:drawable/btn_star" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="66dp"
        android:layout_height="61dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_marginEnd="38dp"
        app:srcCompat="@android:drawable/btn_star_big_off" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="82dp"
        android:layout_toEndOf="@+id/textView5"
        android:text="Note2"
        android:textColor="?android:attr/colorForeground" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView"
        android:layout_toEndOf="@+id/imageButton7"
        android:text="attach a photo"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="132dp"
        android:layout_height="63dp"
        android:layout_alignStart="@+id/textView8"
        android:layout_alignTop="@+id/textView2"
        android:background="#a67339"
        android:text="attach a photo"
        android:textColor="@android:color/background_light" />

    <View
        android:id="@+id/divider5"
        android:layout_width="110dp"
        android:layout_height="3dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="109dp"
        android:layout_toEndOf="@+id/textView5"
        android:background="@android:color/background_light" />

    <TextView
        android:id="@+id/App name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView4"
        android:text="App name"
        android:textColor="@android:color/background_light" />

    <ImageButton
        android:id="@+id/imageButton18"
        android:layout_width="67dp"
        android:layout_height="60dp"
        android:layout_alignStart="@+id/textView4"
        android:layout_alignTop="@+id/imageButton7"
        app:srcCompat="@android:drawable/btn_star_big_on" />

</RelativeLayout>